#include "stdafx.h"
#include <iostream>
#include <conio.h>

using namespace std;

void main()
{
float lado,area;
	cout<<"Ingrese el lado del cuadrado ";
	cin>>lado;
	area=lado*lado;
	cout<<"El lado del cuadrado es "<<area;
	getch();
}
